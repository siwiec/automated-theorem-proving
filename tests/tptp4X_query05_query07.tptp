%     },
%     "where": {
%         "eq": [
%             "a.fst",
%             "a.snd"
%         ]
%     }
% }
fof(main_query_1,definition,(
    ! [FST] :
      ( main_query_1(FST)
    <=> select_1(FST) ) )).

