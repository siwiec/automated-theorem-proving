%             ]
%         },
%         {
%             "lt": [
%                 "a.snd",
%                 "b.snd"
%             ]
%         }
%     ]
% }
fof(where_6,definition,(
    ! [B_FST,B_SND,A_SND,A_FST] :
      ( where_6(B_FST,B_SND,A_SND,A_FST)
    <=> ( eq(A_FST,B_SND)
        & eq(B_FST,A_SND) ) ) )).

