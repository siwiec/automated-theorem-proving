% ]
fof(from_5,definition,(
    ! [A_FST,A_SND,A_TRD,B_FST,B_SND,B_TRD] :
      ( from_5(A_FST,A_SND,A_TRD,B_FST,B_SND,B_TRD)
    <=> ( tab(A_FST,A_SND,A_TRD)
        & tab(B_FST,B_SND,B_TRD) ) ) )).

