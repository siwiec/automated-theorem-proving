% [
%     {
%         "value": "tab",
%         "name": "a"
%     }
% ]
fof(from_2,definition,(
    ! [A_FST,A_SND,A_TRD] :
      ( from_2(A_FST,A_SND,A_TRD)
    <=> tab(A_FST,A_SND,A_TRD) ) )).

% {
%     "eq": [
%         "a.fst",
%         "a.snd"
%     ]
% }
fof(where_3,definition,(
    ! [A_SND,A_FST] :
      ( where_3(A_SND,A_FST)
    <=> eq(A_FST,A_SND) ) )).

% {
%     "select": {
%         "value": "a.fst"
%     },
%     "from": {
%         "value": "tab",
%         "name": "a"
%     },
%     "where": {
%         "eq": [
%             "a.fst",
%             "a.snd"
%         ]
%     }
% }
fof(select_1,definition,(
    ! [A_FST] :
      ( select_1(A_FST)
    <=> ? [A_SND,A_TRD] :
          ( from_2(A_FST,A_SND,A_TRD)
          & where_3(A_SND,A_FST) ) ) )).

% {
%     "select": {
%         "value": "a.fst"
%     },
%     "from": {
%         "value": "tab",
