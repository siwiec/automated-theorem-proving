% {
%     "select": {
%         "value": "a.fst"
%     },
%     "from": {
%         "value": "tab",
%         "name": "a"
%     },
%     "where": {
%         "eq": [
%             "a.fst",
%             "a.snd"
%         ]
%     }
% }
fof(select_1,definition,(
    ! [A_FST] :
      ( select_1(A_FST)
    <=> ? [A_SND,A_TRD] :
          ( from_2(A_FST,A_SND,A_TRD)
          & where_3(A_FST,A_SND) ) ) )).

